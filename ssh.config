Host *
    ServerAliveCountMax 10
    ServerAliveInterval 15
    Compression yes
    ForwardX11Trusted yes

Host ebi_gate
    Hostname ligate.ebi.ac.uk
    Port 2244
    User bletcher
    IdentityFile ~/.ssh/ebi_rsa.pub

